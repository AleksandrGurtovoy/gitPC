<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="619.0" prefWidth="901.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="service.MainWindowService" stylesheets="/media/styles/MainWindowStyle.css">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="683.0" prefWidth="949.0" styleClass="background" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="loginLabel" layoutX="115.0" layoutY="14.0" prefHeight="33.0" prefWidth="273.0" />
                <Label fx:id="nickNameLabel" layoutX="115.0" layoutY="51.0" prefHeight="18.0" prefWidth="273.0" />
                <TextField layoutX="503.0" layoutY="27.0" prefHeight="33.0" prefWidth="252.0" promptText="Search here" />
                <Button id="exitButton" layoutX="828.0" layoutY="27.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="52.0" text="Exit" onAction="#processExit"/>

                <TableView fx:id="tableRepos" layoutY="85.0" prefHeight="534.0" prefWidth="901.0">
                    <columnResizePolicy>
                        <TableView fx:constant = "CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                    <columns>
                        <TableColumn fx:id="numberColumn" prefWidth="70.0" sortable="false" text="#" />
                        <TableColumn fx:id="nameColumn" prefWidth="190.0" sortable="false" text="Name" />
                        <TableColumn fx:id="deskColumn" prefWidth="400.0" sortable="false" text="Description" />
                        <TableColumn fx:id="watchedColumn" sortable="false" text="Watched" />
                        <TableColumn fx:id="starsColumn" sortable="false" text="Stars" />
                        <TableColumn fx:id="forksColumn" sortable="false" text="Forks" />
                    </columns>
                </TableView>
                <ImageView fx:id="photo" fitHeight="68.0" fitWidth="93.0" layoutX="14.0" layoutY="10.0" pickOnBounds="true" preserveRatio="true" />
            <Button layoutX="755.0" layoutY="27.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="31.0" text="Button" />
            </children>
        </AnchorPane>
    </children>
</VBox>
