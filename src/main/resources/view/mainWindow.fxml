<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="619.0" prefWidth="901.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="service.MainWindowService" stylesheets="/media/styles/MainWindowStyle.css">
    <children>
        <AnchorPane id="exitIcon" maxHeight="-1.0" maxWidth="-1.0" prefHeight="683.0" prefWidth="949.0"
                    styleClass="background" VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="loginLabel" layoutX="115.0" layoutY="14.0" prefHeight="33.0" prefWidth="273.0"/>
                <Label fx:id="nickNameLabel" layoutX="115.0" layoutY="51.0" prefHeight="18.0" prefWidth="273.0"/>
                <TextField fx:id="searchField" layoutX="503.0" layoutY="27.0" prefHeight="33.0" prefWidth="252.0"
                           promptText="Search here"/>
                <TableView fx:id="tableRepos" layoutY="85.0" prefHeight="534.0" prefWidth="901.0">
                    <columns>
                        <TableColumn fx:id="numberColumn" editable="false" prefWidth="70.0" sortable="false" text="#"/>
                        <TableColumn fx:id="nameColumn" editable="false" prefWidth="190.0" sortable="false"
                                     text="Name"/>
                        <TableColumn fx:id="deskColumn" editable="false" prefWidth="400.0" sortable="false"
                                     text="Description"/>
                        <TableColumn fx:id="watchedColumn" editable="false" sortable="false" text="Watched"/>
                        <TableColumn fx:id="starsColumn" editable="false" sortable="false" text="Stars"/>
                        <TableColumn fx:id="forksColumn" editable="false" sortable="false" text="Forks"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
                <ImageView fx:id="exitIcon" fitHeight="50.0" fitWidth="53.0" layoutX="825.0" layoutY="19.0"
                           pickOnBounds="true" preserveRatio="true" onMouseClicked="#processExit">
                    <image>
                        <Image url="@../media/dooor.png"/>
                    </image>
                </ImageView>
                <Button layoutX="755.0" layoutY="27.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="31.0"
                        text="Button" onAction="#searchProcess"/>
                <ImageView fx:id="photo" fitHeight="63.0" fitWidth="100.0" layoutX="14.0" layoutY="12.0"
                           pickOnBounds="true" preserveRatio="true"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
